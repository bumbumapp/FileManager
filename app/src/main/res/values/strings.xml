    <string name="intro_hiding_title">Hiding files feature</string>
    <string name="intro_hiding_summary">This icon means, that selected files will be moved to the hidden storage.</string>
    <string name="intro_hiding_summary2">This icon means, that selected files will be moved from the hidden storage to a selected destination.</string>
    <string name="intro_hiding_info"><![CDATA[Files in the hidden storage are accessible <b>only</b> to this app.]]></string>
    <string name="intro_hiding_warning">WARNING: If you uninstall this app or clear app storage, you will also remove all your hidden secrets!</string>

    <string name="intro_encrypting_title">Encrypting files feature</string>
    <string name="intro_encrypting_summary">This icon means, that selected files will be encrypted.</string>
    <string name="intro_encrypting_summary2">This icon means, that selected files will be decrypted.</string>
    <string name="intro_encrypting_info_1"><![CDATA[Files are encrypted <b>without password</b> with key saved securely in your device <i>(in the AndroidKeyStore)</i>.]]></string>
    <string name="intro_encrypting_info_2"><![CDATA[The encryption make possible, that an encrypted files could not be read by an unauthorized person.]]></string>
    <string name="intro_encrypting_warning">WARNING: If you uninstall this app, you will not be able to decrypt your encrypted secrets never again!</string>

    <string name="intro_lock_title">Lock app setup</string>
    <string name="intro_lock_warning">Enabling lock app access is highly recommended to keep your secrets safe.</string>

